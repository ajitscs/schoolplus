Email: <%= f.text_field :email %><br>
First Name: <%= f.text_field :first_name %><br>
Last Name: <%= f.text_field :last_name %><br>
Phone: <%= f.text_field :contact %><br>
<% if current_user.admin? %>
  Role: <%= f.select :role, User::ROLES, selected: f.object.roles.first.try(:name) %><br>
  School: <%= f.select :school_id, School.all.collect{ |u| [u.title, u.id] }, selected: f.object.school %><br>
<% end %>
<%= f.submit %>
